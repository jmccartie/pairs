{
    "type": "FeatureCollection",
        "features": [
		<% @needs.each do |w| %>
		{
			"type": "Feature",
            "geometry": {"type": "Point", "coordinates": [<%= w.affected.contact.address.longitude %>, <%= w.affected.contact.address.latitude %>]},
            "properties": {
                "title": "<%= w.affected.contact.address.address %>"
            }
		}<%= w == @needs.last ? "" : "," %>
		<% end %>
    ]
}